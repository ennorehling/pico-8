pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
sx=0
sy=0
sd=0
f=0
j=0
d=0
s=1
y=10
x=20
ns=0

function _draw()
cls()
print(fl,0,0)
map(0,0,0,0,16,16)
spr(f+s,x,y)
line(0,108,127,108)
if sd~=0 then line(sx,sy,sx+3,sy) end
end

function _update()
tx=flr((x+4)/8)
ty=flr(y/8)+1
t=mget(tx,ty)
fl=fget(t)
-- shot
if sd~=0 then
 sx=sx+sd
end
-- continue jump:
if j>0 then j=j-1 y=y-1
-- fall:
elseif band(fl,1)==0 then y=y+1 end

if band(fl,1)==1 then -- ground
 if btn(4) then j=10 end -- start jump
 if s==3 then s=1 end -- end jump
else -- air
 s=3
end
if btn(0) then
 x=x-1
 f=8
elseif btn(1) then
 x=x+1
 f=0
else
 s=2 -- standing
end

if btn(5) and ns==0 then --shoot
 ns=1
 s=6
 sy=y+5
 if f==0 then
  sx=x+4
  sd=2
 else
  sd=-2
  sx=x-2
 end
elseif not btn(5) then
 ns=0
end

if s==1 or s==2 then
s=3-s -- walk anim
end
end

function _init()
cls()
end

